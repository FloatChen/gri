%gri:# Draw version-history tree
%gri:set font size 12
%gri:.x. = 5				# cm
%gri:.y. = 3				# cm
%gri:.fs. = {rpn ..fontsize.. pttocm} # height of font, in cm
%gri:.space_below. = {rpn .fs. 6 /}	# whitespace below a version name, in cm
%gri:.space_above. = {rpn 0 .fs. 6 / -} # whitespace above
%gri:.vsn_length. = {rpn .fs. 3 *}	# length of vertical inter-version line
%gri:set arrow size 0.1
%gri:
%gri:`version \number \change1 \change2 \change3'
%gri:{
%gri:    draw line from .x. .y. to .x. {rpn .y. .vsn_length. +} cm
%gri:    draw label "\.word4." at {rpn .x. "M" width +} {rpn .y. .space_below. +} cm
%gri:    draw label "\.word3." at {rpn .x. "M" width +} {rpn .y. .space_below. .fs. + +} cm
%gri:    draw label "\.word2." at {rpn .x. "M" width +} {rpn .y. .space_below. .fs. 2 * + +} cm
%gri:    .y. += {rpn .vsn_length. .space_below. +}
%gri:    draw label "\.word1." centered at .x. .y. cm
%gri:    .y. += {rpn .fs. .space_above. +}
%gri:}
%gri:`from \version'
%gri:{
%gri:    draw arrow from {rpn .x. .fs. 6 * -} {rpn .y. .fs. 2 / +} to {rpn .x. .fs. - } {rpn .y. .fs. 2 / +} cm
%gri:    draw label "\.word1." at {rpn .x. .fs. 5 * -} {rpn .y. .fs. 1.5 / +} cm
%gri:}
%gri:`name \text'
%gri:{
%gri:    set font size {rpn ..fontsize.. 1.5 *}
%gri:    draw label "\.word1." at .x. .y. cm
%gri:    .y. += {rpn .fs. 2 *}
%gri:    set font size {rpn ..fontsize.. 1.5 /}
%gri:}
